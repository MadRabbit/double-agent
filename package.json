{
  "name": "doubleagent",
  "version": "2.0.0",
  "description": "A superagent based async/await friendly testing util",
  "files": [
    "dist"
  ],
  "main": "dist/doubleagent.js",
  "types": "dist/doubleagent.d.ts",
  "scripts": {
    "prepare": "rm -rf dist && tsc",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "test": "NODE_ENV=test mocha"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadRabbit/doubleagent.git"
  },
  "keywords": [
    "http",
    "tdd",
    "bdd",
    "async",
    "await",
    "test",
    "superagent"
  ],
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadRabbit/doubleagent/issues"
  },
  "homepage": "https://github.com/MadRabbit/doubleagent#readme",
  "dependencies": {
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/superagent": "^4.1.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint-config-compono-ts": "^1.3.0",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mocha": "^10.2.0",
    "qs": "^6.9.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
